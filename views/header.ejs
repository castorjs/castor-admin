<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %> - Castor Admin</title>

    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/bootstrap-theme.min.css">
    <link rel="stylesheet" href="/javascripts/jQuery-Notify-bar/jquery.notifyBar.css">
    <link rel="stylesheet" href="/stylesheets/style.css">


    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script> -->
    <![endif]-->
    
    <script type="text/javascript" src="/javascripts/jquery.min.js"></script>
    <script type="text/javascript" src="/javascripts/bootstrap.min.js"></script>

  </head>
  <body>


    <nav class="navbar navbar-inverse navbar-fixed-top">

        <ul class="nav navbar-nav col-lg-6" id="ulHeaderLeft">
            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </a>
            <li>
                <a href="/" class="navbar-brand" id="home">
                    <span class="glyphicon glyphicon-home"></span>
                    CASTOR
                </a>
            </li>
            <% var paths = {'/upload': 'Upload'} %>
            <% var usedPath = '' %>
            <% if (typeof path !== 'undefined') { %>
              <% usedPath = path %>
            <% } %>
            <% for (var p in paths) { %>
              <% label = paths[p] %>
                <li  <% if (p === (usedPath || '')) { %>  class="active"   <% } %>  >
                  <a href="<%= p %>"><span class="glyphicon glyphicon-upload"></span> <%= label %></a>
                </li>
            <% } %>
        </ul>

        <ul class="nav navbar-nav col-lg-6" id="ulHeaderRight">
            <li><a href="#"><%= userName %></a></li>
        </ul>

    </nav>

    <div class="container-fluid" id="container">
